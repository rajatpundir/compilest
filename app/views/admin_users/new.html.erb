<% @page_title = "Create User"%>
<div class="center page-header">
  <h2>User Registeration Form</h2>
  <h5>Please take your time to fill all fields correctly.</h5>
  <h5>Incomplete forms will be rejected by the system.</h5>
</div>
<%= form_for @admin_user, html: { class: 'form-horizontal', role: 'form' } do |f| %>
    <%= render(:partial => 'form', :locals => {:f => f}) %>
    <div class="form-group">
      <div class="col-sm-offset-2 col-sm-10">
        <%= f.submit "Register",class: 'btn btn-primary btn-lg' %>
      </div>
    </div>
<% end %>
